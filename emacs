;Load the hooks
(load "/home/nate/.emacs.d/custom/hooks.el")

;Custom set certain settings (scroll bar, character indicator, etc)
(load "/home/nate/.emacs.d/custom/settings.el")

;Set the autosave rules
(load "/home/nate/.emacs.d/custom/rules.el")

;Run every other kind of startup function
(load "/home/nate/.emacs.d/custom/startup.el")

